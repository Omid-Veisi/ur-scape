def cost(x):
    x[0] *= 180
    df = pd.DataFrame(data=x, columns=['vars'])
    df.to_csv('variables.csv')
    
    sleep(1)
    
    cost = pd.read_csv('from_grasshopper.csv')
    
    return cost['cost'][0]
    
    
